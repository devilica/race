{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(.*\\.php)",
            "dest": "/public/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}